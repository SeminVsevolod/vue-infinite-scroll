{
  "name": "vue-infinite-scroll",
  "version": "0.1.0",
  "author": {
    "name": "Semin Vsevolod"
  },
  "license": "MIT",
  "main": "dist/vue-infinite-scroll.umd.js",
  "module": "dist/vue-infinite-scroll.esm.js",
  "unpkg": "dist/vue-infinite-scroll.min.js",
  "browser": {
    "./sfc": "src/components/vue-infinite-scroll.vue"
  },
  "files": [
    "dist/*",
    "src/*",
    "attributes.json",
    "tags.json"
  ],
  "vetur": {
    "tags": "tags.json",
    "attributes": "attributes.json"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-infinite-scroll.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-infinite-scroll.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-infinite-scroll.min.js"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.14.4",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
